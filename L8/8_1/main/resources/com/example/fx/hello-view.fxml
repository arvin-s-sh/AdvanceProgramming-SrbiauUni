<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.fx.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Label text="real name : "/><TextField fx:id="txtRealName"/>
    <Label text="work experience : "/><TextField fx:id="txtExp"/>
    <Label text="age : "/><TextField  fx:id="txtAge"/>
    <Label text="email : "/><TextField fx:id="txtEmail"/>
    <Button text="submit" onAction="#insert"/>

    <Label fx:id="loginText"/>
</VBox>
