<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<VBox fx:id="edit_form" alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.fx.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Label text="user name : "/><TextField fx:id="txtUserName" text="arvin"/>
    <Label text="password : " /><TextField fx:id="txtPass" text="1381"/>
    <Label text="email : " /><TextField  fx:id="txtEmail" text="arvin@gmail.com"/>
    <Button text="update" onAction="#update" />

    <Label fx:id="loginText"/>
</VBox>


